				$('.mainTable').not(':first').find('.tablerow').toggle();
				$('.TableToggle').click(function(){
				    var a = $(this).closest('tr');
				   	a.next().nextUntil('tr:has(.tablerow)').toggle();
				});
						
			});
		})(jQuery);
    </script>
